{
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "files.associations": {
    "**/{{ cookiecutter.project_slug }}/**/*.py": "jinja-py",
    "**/hooks/*_gen_project.py": "jinja-py"
  },
  "files.exclude": {
    "**/.git": true
  },
  "python.linting.pylintEnabled": true
}
